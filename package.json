{
    "name": "close-all",
    "displayName": "Close All",
    "description": "Adds a close all button to the editor title",
    "version": "1.0.0",
    "publisher": "bpasero",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.closeAll"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.closeAll",
            "title": "Close All Editors in Group",
            "icon": {
                "light": "./media/closeall.svg",
                "dark": "./media/closeall_inverse.svg"
            }
        }],
        "menus": {
            "editor/title": [
                {
                    "command": "extension.closeAll",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}